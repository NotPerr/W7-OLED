#define btnPin 14
int mode = 0;
bool btnPress = false;

void setup() {
  // put your setup code here, to run once:
   //OLED define
   pinMode(btnPin, INPUT);
   Serial.begin(115200);
   display.init();
   display.flipScreenVertically();
   display.setContrast(255); //數值介於0~255，調整對比
   display.clear();//清除螢幕和緩衝區(clear)
}

void loop() {
  // put your main code here, to run repeatedly:
  if(mode == 0){
  Blink();
  }
  else if(mode == 1) smile();
  
  if(digitalRead(btnPin) == HIGH &&btnPress == false){
    mode += 1;
    if(mode > 1) mode = 0;
    }
   else if (digitalRead(btnPin) == LOW && btnPress ==true){
    btnPress = false;
   }
  delay(20);
  Serial.println(mode);
}
